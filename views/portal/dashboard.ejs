<html class="no-js" lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Uthabiti Africa | Dashboard</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Top script start -->
      <%- include('../partials/top_script'); %>
    <!-- / Top script end -->
</head>

<body>
    <!-- Side bar -->
     <%- include('../partials/portal_sidebar'); %>
     <!-- / Side bar -->

    <!-- Start Welcome area -->
    <div class="all-content-wrapper">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="logo-pro">
                        <a href="/portal/dashboard"><img class="main-logo" src="/img/logo/logo.png" height="30px" width="80px" alt="" /></a>
                    </div>
                </div>
            </div>
        </div>
        <div class="header-advance-area">
            <!-- Header top start -->
             <%- include('../partials/header_top'); %>
            <!-- / Header top end -->
            <!-- Mobile Menu start -->
            <%- include('../partials/mobile_menu'); %>
            <!-- Mobile Menu end -->
             
            <div class="breadcome-area">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <div class="breadcome-list">
                                <div class="row">
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <div class="breadcome-heading">
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                        <ul class="breadcome-menu">
                                            <li><a href="#">Home</a> <span class="bread-slash">/</span>
                                            </li>
                                            <li><span class="bread-blod">Dashboard</span>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="section-admin container-fluid res-mg-t-15">
            <div class="row admin text-center">
                
                <div class="col-md-12">
                    <div class="row">
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">
                            <div class="admin-content analysis-progrebar-ctn">
                                <h4 class="text-left text-uppercase"><b>Beneficiaries</b></h4>
                                <div class="row vertical-center-box vertical-center-box-tablet">
                                    <div class="col-xs-3 mar-bot-15 text-left">
                                        <label class="label bg-brown">%0 <i class="fa fa-level-down" aria-hidden="true"></i></label>
                                    </div>
                                    <div class="col-xs-9 cus-gh-hd-pro">
                                        <h2 class="text-right no-margin">
                                            <%= parseFloat(facilityPeople.totalBeneficiary).toLocaleString(undefined, { minimumFractionDigits: 0,
                                    maximumFractionDigits: 0 }) %>
                                        </h2>
                                    </div>
                                </div>
                                <div class="progress progress-mini">
                                    <div style="width: 0%;" class="progress-bar bg-red"></div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12" style="margin-bottom:1px;">
                            <div class="admin-content analysis-progrebar-ctn res-mg-t-30">
                                <h4 class="text-left text-uppercase"><b>Facilities</b></h4>
                                <% 
                                let f_change = parseFloat(facilityStats.percentageChangeFT); 
                                let f_trendClass = "bg-secondary";
                                let f_icon = "";

                                if (facilityStats.facility_trend === "Increase") {
                                    f_trendClass = "bg-light-blue"; 
                                    f_icon = "fa fa-level-up";
                                } else if (facilityStats.facility_trend === "Decrease") {
                                    f_trendClass = "bg-red"; 
                                    f_icon = "fa fa-level-down";
                                }
                                %>
                                <div class="row vertical-center-box vertical-center-box-tablet">
                                    <div class="text-left col-xs-3 mar-bot-15">
                                        <label class="label <%= f_trendClass %>"><%= facilityStats.percentageChangeFT %> <i class="<%= f_icon %>" aria-hidden="true"></i></label>
                                    </div>
                                    <div class="col-xs-9 cus-gh-hd-pro">
                                        <h2 class="text-right no-margin">
                                            <%= parseFloat(facilityStats.total_facility).toLocaleString(undefined, { minimumFractionDigits: 0,
                                    maximumFractionDigits: 0 }) %>
                                        </h2>
                                    </div>
                                </div>
                                <div class="progress progress-mini">
                                    <div style="width: <%= facilityStats.percentageChangeFT %>;" class="progress-bar progress-bar-danger <%= f_trendClass %>"></div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">
                            <div class="admin-content analysis-progrebar-ctn res-mg-t-30">
                                <h4 class="text-left text-uppercase"><b>Members</b></h4>
                                <% 
                                let m_change = parseFloat(membersStats.percentageChangeMB); 
                                let m_trendClass = "bg-secondary";
                                let m_icon = "";

                                if (membersStats.member_trend === "Increase") {
                                    m_trendClass = "bg-green"; 
                                    m_icon = "fa fa-level-up";
                                } else if (membersStats.member_trend === "Decrease") {
                                    m_trendClass = "bg-red"; 
                                    m_icon = "fa fa-level-down";
                                }
                                %>
                                <div class="row vertical-center-box vertical-center-box-tablet">
                                    <div class="text-left col-xs-3 mar-bot-15">
                                        <label class="label <%= m_trendClass %>"><%= membersStats.percentageChangeMB %> <i class="<%= m_icon %>" aria-hidden="true"></i></label>
                                    </div>
                                    <div class="col-xs-9 cus-gh-hd-pro">
                                        <h2 class="text-right no-margin">
                                            <%= parseFloat(membersStats.total_member).toLocaleString(undefined, { minimumFractionDigits: 0,
                                    maximumFractionDigits: 0 }) %>
                                        </h2>
                                    </div>
                                </div>
                                <div class="progress progress-mini">
                                    <div style="width: <%= membersStats.percentageChangeMB %>;" class="progress-bar <%= m_trendClass %>"></div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-12">
                            <div class="admin-content analysis-progrebar-ctn res-mg-t-30">
                                <h4 class="text-left text-uppercase"><b>Careggivers</b></h4>
                                <div class="row vertical-center-box vertical-center-box-tablet">
                                    <div class="text-left col-xs-3 mar-bot-15">
                                        <label class="label bg-purple">0% <i class="fa fa-level-up" aria-hidden="true"></i></label>
                                    </div>
                                    <div class="col-xs-9 cus-gh-hd-pro">
                                        <h2 class="text-right no-margin"><%= parseFloat(facilityPeople.totalCaregiver).toLocaleString(undefined, { minimumFractionDigits: 0,
                                    maximumFractionDigits: 0 }) %></h2>
                                    </div>
                                </div>
                                <div class="progress progress-mini">
                                    <div style="width: 0%;" class="progress-bar bg-purple"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="product-sales-area mg-tb-30">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <iframe title="Uthabiti_Analytics" width="100%" height="541.25" src="https://app.powerbi.com/reportEmbed?reportId=68449eea-3279-4251-af79-3825bb03d0ac&autoAuth=true&ctid=41bf6d7d-709a-43f6-93ed-6cff93d7fd2e" frameborder="0" allowFullScreen="true"></iframe>
                    </div>
                </div>
            </div>
        </div>

